#include <stdio.h>
#include <stdlib.h>

int main(void)
{
char *name1;
char *name2;
int name1_len, name2_len;

printf("Enter the length of name1: ");
scanf("%d", &name1_len);
printf("Enter the length of name2: ");
scanf("%d", &name2_len);

name1 = (char * )malloc((name1_len)*sizeof(char));
name2 = (char * )malloc((name2_len)*sizeof(char));

printf("Enter name1: ");
scanf("%s", & * name1);
printf("Enter name 2: ");
scanf("%s", & * name2);

printf("The contributors to this project work are: %s and %s\n", name1, name2);
return (0);
}
